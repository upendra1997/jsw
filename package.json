{
  "name": "jsw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "client": "cd client && npm start",
    "server": "node ./bin/www",
    "devserver": "nodemon ./bin/www",
    "devstart-windows": "concurrently --kill-others-on-fail \"npm run devserver\" \"npm run client\"",
    "devstart-linux": "npm run devserver& npm run client",
    "database": "mongod --dbpath ./database &",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dataframe-js": "1.3.2",
    "debug": "2.6.9",
    "express": "4.16.0",
    "express-fileupload": "0.4.0",
    "http-errors": "1.6.2",
    "jsonwebtoken": "8.2.1",
    "lodash": "4.17.10",
    "mailgun": "^0.5.0",
    "mailgun-js": "^0.18.1",
    "mongoose": "5.1.1",
    "morgan": "1.9.0",
    "nodemailer": "4.6.7",
    "papaparse": "4.4.0",
    "pug": "2.0.0-beta11",
    "validator": "10.2.0"
  },
  "devDependencies": {
    "concurrently": "3.5.1",
    "nodemon": "1.17.5"
  }
}
